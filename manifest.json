{
  "manifest_version": 2,

  "name": "MI Tech - Channel Advisor Token",
  "description": "Retrieve Channel Advisor Token history by Edgar Ronda.",
  "version": "1.0",

  "author": "Edgar Ronda",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",

  "icons": { "16": "/assets/icon16x16.png",
            "48": "/assets/icon48x48.png",
            "128": "/assets/icon128x128.png" 
            },

  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/",
    "http://apps3.mitechnologiesinc.com/",
    "http://192.168.0.65:8089/api/v1/activetoken/",
    "unlimitedStorage",
    "notifications"
    ],
   "background": { "scripts": ["jquery.min.js","moment.min.js","background.js"] },

  "web_accessible_resources": [
    "assets/icon48x48.png"
  ],

   "homepage_url": "http://apps3.mitechnologiesinc.com/"
}